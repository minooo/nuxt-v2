{
  "name": "2-nuxt-v2",
  "version": "1.0.0",
  "description": "this is minooo's app description......",
  "author": "minooo",
  "private": true,
  "main": "main.js",
  "build": {
    "productName": "my-electron-vue",
    "appId": "com.example.xiangju",
    "directories": {
      "output": "build",
      "buildResources": "static"
    },
    "copyright": "Copyright © 2018 象聚",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "static/icons/icon.icns",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "icon": "static/icons/icon.ico",
      "target": [
        "nsis",
        "zip"
      ]
    },
    "linux": {
      "icon": "static/icons"
    }
  },
  "scripts": {
    "dev:web": "cross-env NODE_ENV=development NODE_WEB=web nodemon server/index.js --watch server",
    "dev": "cross-env NODE_ENV=development electron .",
    "build:web": "cross-env NODE_WEB=web nuxt build",
    "build": "nuxt build",
    "build:win": "nuxt build && electron-builder -w",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "cross-env": "^5.2.0",
    "crypto-browserify": "^3.12.0",
    "element-ui": "^2.4.8",
    "koa": "^2.5.2",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "nuxt": "^2.2.0",
    "qs": "^6.5.2",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "electron": "^3.0.5",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.6",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "nodemon": "^1.11.0",
    "prettier": "1.14.3"
  }
}
